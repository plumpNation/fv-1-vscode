{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "FV-1 Assembly",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [{
        "name": "comment.line.fv-1-assembly",
        "match": "^;.+"
      }]
    },
    "keywords": {
      "patterns": [{
        "name": "keyword.control.fv-1-assembly",
        "match": "(?i)\\b(equ|mem|rda|rmpa|wra|wrap|rdax|rdfx|wrax|wrhx|wrlx|maxx|absa|mulx|log|exp|sof|and|or|xor|skp|wlds|wldr|jam|cho|nop|not|clr)\\b"
      }]
    },
    "strings": {
      "name": "string.quoted.double.fv-1-assembly",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.fv-1-assembly",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.spn"
}